{
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "react-router build",
    "db:start": "npx supabase start -x storage-api,imgproxy,edge-runtime,logflare,supavisor,vector",
    "dev": "react-router dev",
    "format": "oxfmt --write .",
    "format:check": "oxfmt --check .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "react-router-serve ./build/server/index.js",
    "test": "vitest --coverage",
    "test:coverage": "open coverage/index.html; vitest --coverage",
    "test:e2e:dev": "PW_USE_DEV_SERVER=1 npx playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "npx playwright test",
    "typecheck": "react-router typegen && tsc -b",
    "validate": "run-p \"test -- --run\" format lint typecheck test:e2e:run"
  },
  "dependencies": {
    "@fontsource-variable/caveat": "^5.2.8",
    "@fontsource-variable/inter": "^5.2.8",
    "@mjackson/form-data-parser": "^0.9.1",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@react-router/fs-routes": "^7.0.0",
    "@react-router/node": "^7.0.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/vite": "^4.1.18",
    "@vercel/analytics": "^1.6.1",
    "clsx": "^2.1.1",
    "immer": "^11.1.4",
    "isbot": "^4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-dropzone": "^15.0.0",
    "react-router": "^7.0.0",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.58.2",
    "@react-router/dev": "^7.0.0",
    "@react-router/serve": "^7.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv": "^17.3.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.6.1",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.33.0",
    "supabase": "^2.76.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom",
    "vite": "$vite"
  },
  "engines": {
    "node": ">=20"
  }
}
