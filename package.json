{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "db:start": "npx supabase start -x storage-api,imgproxy,postgres-meta,edge-runtime,logflare,supavisor,vector",
    "dev": "remix dev",
    "format": "prettier --write . --log-level silent",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "node --require dotenv/config node_modules/@remix-run/serve/dist/cli.js build/index.js",
    "test": "vitest --coverage",
    "test:coverage": "open coverage/index.html; vitest --coverage",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" format lint typecheck test:e2e:run"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@remix-run/node": "^2.17.4",
    "@remix-run/react": "^2.17.4",
    "@supabase/supabase-js": "^2.95.3",
    "@vercel/analytics": "^1.6.1",
    "@vercel/node": "^5.6.3",
    "classnames": "^2.5.1",
    "immer": "^11.1.4",
    "nanoid": "^5.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-dropzone": "^15.0.0",
    "recharts": "^3.7.0",
    "use-fit-text": "^2.4.0",
    "use-sound": "^4.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.17.4",
    "@remix-run/serve": "^2.17.4",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "cross-env": "^10.1.0",
    "cypress": "^15.10.0",
    "dotenv": "^17.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^20.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "start-server-and-test": "^2.1.3",
    "supabase": "^2.76.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "engines": {
    "node": ">=18"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/api"
  ]
}
